dimensions      [0 1 0 0 0 0 0];  // Dimension of length [m]

internalField   uniform 1.0;      // Default value outside the structure

boundaryField
{
    // Define boundary conditions for each patch
    // These should match the patches defined in your blockMeshDict
    
