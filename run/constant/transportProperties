FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Etching parameters
etchRate        etchRate [0 1 -1 0 0 0 0] 0.05;  // Etching rate in m/s

// Level set parameters
reInitInterval  5;  // Number of time steps between reinitialization

